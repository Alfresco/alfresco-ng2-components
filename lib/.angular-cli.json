{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "lib"
  },
  "apps": [
    {
      "name": "core",
      "root": "./",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        { "glob": "assets/app.config.json", "input": "./", "output": "./" },
        { "glob": "core/assets", "input": "./", "output": "./assets" },
        { "glob": "content-services/assets", "input": "./", "output": "./assets" },
        { "glob": "process-services/assets", "input": "./", "output": "./assets" }
      ],
      "index": "index.html",
      "main": "core/index.ts",
      "test": "core/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "content-services",
      "root": "./",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        { "glob": "assets/app.config.json", "input": "./", "output": "./" },
        { "glob": "core/assets", "input": "./", "output": "./assets" },
        { "glob": "content-services/assets", "input": "./", "output": "./assets" },
        { "glob": "process-services/assets", "input": "./", "output": "./assets" }
      ],
      "index": "index.html",
      "main": "content-services/index.ts",
      "test": "content-services/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "process-services",
      "root": "./",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        { "glob": "assets/app.config.json", "input": "./", "output": "./" },
        { "glob": "core/assets", "input": "./", "output": "./assets" },
        { "glob": "content-services/assets", "input": "./", "output": "./assets" },
        { "glob": "process-services/assets", "input": "./", "output": "./assets" }
      ],
      "index": "index.html",
      "main": "process-services/index.ts",
      "test": "process-services/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "./tsconfig.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
