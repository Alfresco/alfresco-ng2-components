{
  "name": "ng2-alfresco-datatable-demo",
  "description": "Alfresco Angular2 DataTable Component - Demo",
  "version": "0.1.0",
  "author": "Alfresco Software, Ltd.",
  "main": "index.js",
  "scripts": {
    "clean": "npm install rimraf && npm run clean-build && rimraf dist node_modules typings dist",
    "clean-build" : "rimraf 'src/{,**/}**.js' 'src/{,**/}**.js.map' 'src/{,**/}**.d.ts'",
    "postinstall": "npm run build",
    "start": "npm run build && concurrently \"npm run tsc:w\" \"npm run server\" ",
    "server": "wsrv -o -s -l",
    "build": "npm run tslint && npm run clean-build && npm run tsc",
    "build:w": "npm run tslint && rimraf dist && npm run tsc:w",
    "travis": "npm link ng2-alfresco-core",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tslint": "tslint -c tslint.json *.ts && tslint -c tslint.json src/{,**/}**.ts  -e '{,**/}**.d.ts'"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Denys Vuika",
      "email": "denis.vuyka@gmail.com"
    }
  ],
  "dependencies": {
    "@angular/common": "2.2.2",
    "@angular/compiler": "2.2.2",
    "@angular/compiler-cli": "2.2.2",
    "@angular/core": "2.2.2",
    "@angular/forms": "2.2.2",
    "@angular/http": "2.2.2",
    "@angular/platform-browser": "2.2.2",
    "@angular/platform-browser-dynamic": "2.2.2",
    "@angular/material": "2.0.0-beta.1",
    "@angular/router": "3.2.2",
    "@angular/upgrade": "2.2.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23",
    "intl": "1.2.4",
    "dialog-polyfill": "^0.4.3",
    "element.scrollintoviewifneeded-polyfill": "^1.0.1",
    "material-design-icons": "2.2.3",
    "material-design-lite": "1.2.1",
    "ng2-translate": "2.5.0",
    "alfresco-js-api": "~1.1.0",
    "ng2-alfresco-core": "1.1.0",
    "ng2-alfresco-datatable": "1.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.33",
    "@types/node": "^6.0.42",
    "concurrently": "^2.2.0",
    "rimraf": "2.5.2",
    "tslint": "3.15.1",
    "typescript": "^2.0.3",
    "wsrv": "^0.1.5"
  },
  "keywords": [
    "angular2",
    "typescript"
  ],
  "license-check-config": {
    "src": [
      "**/*.js",
      "**/*.ts",
      "!/**/coverage/**/*",
      "!/**/demo/**/*",
      "!/**/node_modules/**/*",
      "!/**/typings/**/*",
      "!*.js"
    ],
    "path": "assets/license_header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  }
}
